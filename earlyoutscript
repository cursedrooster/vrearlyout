// Get the current date
var currentDate = new Date();

// Set the time to noon (12:00:00)
currentDate.setHours(12, 0, 0, 0);

// Calculate the time difference between the current time and noon
var timeDifference = currentDate.getTime() - Date.now();

// Start the countdown timer
var eolist = setTimeout(function() {
    // Execute the code
    document.querySelector('.action_eo_list').click();

    // Define a function to check if the button is available
    function submit() {
        var button = document.querySelector('input.button.affirmative');
        if (button) {
            button.click(); // Click the button if it's available
            clearInterval(intervalId); // Stop checking once the button is clicked
        }
    }

    // Set an interval to check every x ms
    var intervalId = setInterval(submit, 10);
}, timeDifference);
